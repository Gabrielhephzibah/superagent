<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cherish.superagent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.TransactionHistoryActivity"></activity>
        <activity android:name=".activities.Withdraw_Successful" />
        <activity android:name=".activities.withdrawal_confirmationPage" />
        <activity android:name=".activities.Withdrawal_InputPin" />
        <activity android:name=".activities.Withdrawal_Page" />
        <activity android:name=".activities.Deposit_Successful" />
        <activity android:name=".activities.Confirmation" />
        <activity android:name=".activities.Deposit" />
        <activity android:name=".activities.Homeview" />
        <activity android:name=".activities.PinSet" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>